# Expected output:
#
# Before ctx
# Init
# Enter
# Hello, World
# Exit

let ctx = () => {
  class CtxManager:
    def __init__(self):
      print('Init')
    def __enter__(self):
      print('Enter')
      return self
    def __exit__(self, *a):
      print('Exit')
    def say_hello(self, name):
      print('Hello,', name)
  return CtxManager
}()

print('Before ctx')
ctx {
  say_hello('World')
}
